package edu.lmu.cmsi.gabriel.recursionandstacks.symbolbalancer;

import java.util.ArrayDeque;
import java.util.Deque;

public class SymbolBalancer {
	Deque<Character> symbolStack = new ArrayDeque<Character>();
	char[] stringChars;
	
	public SymbolBalancer(String string){
		stringChars = new char[string.length()];
		stringChars = string.toCharArray();
		
	}
}
